/* search.wxss*/
.search-input-box{
  height: 70rpx;
}
.search-input{
  width: 80%;
  border-radius: 10rpx 0 0 10rpx;
  background: #efefef;
  padding-left: 12rpx;
  float: left;
}
.search-input .left{
  width: 33rpx;
  line-height: 70rpx;
}
.search-input .middle{
 flex: 1;
}
.search-input input{
  font-size: 28rpx;
}
.search-btn{
  width: 20%;
}
.search-btn button{
  border-radius: 0 10px 10px 0;
  font-size: 28rpx;
}
.search-btn button,.search-input input{
  box-sizing: border-box;
  height: 70rpx;
  line-height: 70rpx;
}

.search-cont-box{
  margin-top: 30rpx;
}
.title-box{
  font-size: 26rpx;
  color: #999;
}
.search-history-list{
  padding: 30rpx 0;
}
.search-history-li{
  width: 33.3%;
  padding: 0 5rpx;
  box-sizing: border-box;
  float: left;
}
.history-item{
  font-size: 26rpx;
  padding:8rpx 0;
  border: 1px solid #eee;
  border-radius:30rpx;
  width:100%;  
  box-sizing: border-box;
  margin-bottom: 15rpx;
}

.rearch-result-box{
  width: 100%; height: 100%;
  position: relative;
  box-sizing: border-box;
  border: 1px solid #efefef;
  background: #fff;
  z-index: 10;
}
.rearch-result-item{
  font-size: 28rpx;
  color: #999;
  padding: 5rpx 0;
  padding-left: 15rpx;
}